[
  {
    "description": "这是一个会返回cookies信息的get请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "成功获得cookies信息",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  },
  {
    "description":"这是一个带cookie的post请求",
    "request":{
      "uri": "/get/with/Cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "wangenhui",
        "age": "22"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "wangenhui": "success",
        "status": "1"
      }
    }
  },
  {
    "description":"这是一个带cookie的post请求",
    "request":{
      "uri": "/post/with/Cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "wangenhui",
        "age": "22"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "wangenhui": "success",
        "status": "1"
      }
    }
  }
